{
  "name": "hello_world_sam_node_starter",
  "version": "1.0.0",
  "description": "Hello World starter for Node.js",
  "main": "app.js",
  "repository": "https://github.com/fourTheorem/hello-world-sam-node-starter",
  "type": "module",
  "author": "fourTheorem",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.5.1",
    "@aws-lambda-powertools/metrics": "^1.5.1",
    "@aws-lambda-powertools/tracer": "^1.5.1",
    "@middy/core": "^4.2.5",
    "@middy/http-error-handler": "^4.2.6"
  },
  "scripts": {
    "test": "tap --ts --test-regex='/tests/.+\\.test\\.ts'",
    "lint": "eslint ."
  },
  "tap": {
    "node-arg": [
      "--no-warnings",
      "--loader=ts-node/esm",
      "--experimental-specifier-resolution=node",
      "--trace-deprecation"
    ]
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.110",
    "@types/tap": "^15.0.8",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.53.0",
    "esbuild": "^0.17.10",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}